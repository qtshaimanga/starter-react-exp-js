{
  "name": "threejs-boilerplate",
  "version": "1.0.0",
  "description": "Experiment boilerplate with three.js, Wagner, Webpack 2 & gsap",
  "main": "client/App.js",
  "scripts": {
    "start": "NODE_ENV=DEV webpack-dev-server --config webpack/webpack.dev.config.babel.js",
    "build": "NODE_ENV=PROD webpack --config webpack/webpack.prod.config.babel.js --progress --profile --colors"
  },
  "repository": "git+https://github.com/avanderpotte/threejs-boilerplate.git",
  "keywords": [
    "threejs",
    "boilerplate",
    "es6",
    "webpack",
    "wagner",
    "gsap",
    "stylus"
  ],
  "author": "Adrien VANDERPOTTE <adrien.vanderpotte@gmail.com> (http://adrien-vanderpotte.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/avanderpotte/threejs-boilerplate/issues"
  },
  "homepage": "https://github.com/avanderpotte/threejs-boilerplate#readme",
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "clean-webpack-plugin": "^0.1.15",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.2",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "glslify-fancy-imports": "^1.0.1",
    "glslify-hex": "^2.1.1",
    "glslify-loader": "^1.0.2",
    "html-webpack-plugin": "^2.28.0",
    "ify-loader": "^1.0.3",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.13.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "2"
  },
  "dependencies": {
    "@superguigui/wagner": "^0.1.5",
    "dom-hand": "^1.0.2",
    "domready": "^1.0.8",
    "event-emitter": "^0.3.4",
    "glslify": "^6.0.1",
    "gsap": "^1.19.1",
    "howler": "^2.0.2",
    "raf-loop": "^1.1.3",
    "stats.js": "^0.17.0",
    "three": "^0.84.0",
    "three-orbit-controls": "^82.1.0"
  },
  "glslify": {
    "transform": [
      "glslify-fancy-imports",
      "glslify-hex"
    ]
  }
}
